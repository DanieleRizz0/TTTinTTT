<%-include("header.ejs")%>
    <div class="macro-container">
        
        <h1 class="title">
            Tic Tac Toe in Tic Tac <button class="reset" type="button">RESET</button> <button class="edit" type="button">EDIT</button>
        </h1>
        
        
        <% for( let index1 = 0; index1 < 9; index1++ ) { %>
                <div class="sub-container" id="<%=index1%>">
                    <% for( let index2 = 0; index2 < 9; index2++ ) { %>
                        <div class="square-item" id="<%= index1 %>-<%= index2 %>"></div>
                    <% } %>
                </div>
        <% } %>
    </div>
<%-include("footer.ejs")%>